function chunkSubstr(str, size) {&#xd;
	var numChunks = Math.ceil(str.length() / size);&#xd;
	var chunks = new Array(numChunks);&#xd;
&#xd;
	for (i = 0, o = 0; i &lt; numChunks; ++i, o += size) {&#xd;
		chunks[i] = str.substr(o, size)&#xd;
	}&#xd;
	return chunks&#xd;
}&#xd;
