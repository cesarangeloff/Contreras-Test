	for (i = 0; i &lt; 30; i++) {&#xd;
		jsonData[&apos;jsonModel_&apos; + (i + 1)] = i &lt; arr.length ? arr[i] : &apos;&apos;&#xd;
	}&#xd;
	log.info(&apos;Form data saved.&apos;)&#xd;
	return jsonData&#xd;
